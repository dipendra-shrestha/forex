pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building stage ..'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing stage..'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Deploying stage..'
            }
        }
        stage('Clean up') {
            steps {
                echo 'Cleanup stage..'
            }
        }
    }
}
